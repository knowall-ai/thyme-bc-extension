# Business Central Configuration
# Copy this file to .env and fill in your values
# NEVER commit the .env file - it contains secrets

# Azure AD tenant ID (from Azure Portal or `az account show --query tenantId`)
BC_TENANT_ID=your-tenant-id-here

# Azure AD App Registration (create in Azure Portal > App registrations)
BC_CLIENT_ID=your-client-id-here
BC_CLIENT_SECRET=your-client-secret-here

# Target BC environment name
BC_ENVIRONMENT=Sandbox

# Company name in BC
BC_COMPANY=CRONUS UK Ltd
